{
  "name": "vite-ant-mobile",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "patch-package",
    "prepare": "husky install",
    "tsc:init": "tsc --init",
    "svgo": "svgo -f src/icons",
    "readme:v1": "echo 'Vite starts the react service'",
    "dev": "vite --host",
    "preview": "vite preview --host",
    "build": "tsc && vite build",
    "readme:v2": "echo 'test lint...'",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.vue --ignore-pattern \"bad.*\" --max-warnings 0",
    "lint:src": "eslint ./src --ext .js,.jsx,.ts,.tsx,.vue",
    "lint:fix": "eslint ./src --ext .js,.jsx,.ts,.tsx,.vue --ignore-pattern \"bad.*\" --fix",
    "prettier": "prettier -l \"./src/*\"",
    "prettier:fix": "prettier --write -l \"./src/*\""
  },
  "dependencies": {
    "@nplayer/react": "^1.0.9",
    "amfe-flexible": "^2.2.1",
    "antd-mobile": "^5.0.0-beta.2",
    "gsap": "^3.7.1",
    "nplayer": "^1.0.9",
    "pdfjs-dist": "^2.9.359",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.1",
    "react-virtualized": "^9.22.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/node": "^16.7.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-config": "^5.0.3",
    "@types/react-router-dom": "^5.1.8",
    "@types/react-virtualized": "^9.21.13",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "@vitejs/plugin-legacy": "^1.5.2",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "autoprefixer": "^10.3.4",
    "babel-eslint": "^10.1.0",
    "commitizen": "^4.2.4",
    "eslint": "^7.32.0",
    "eslint-config-alloy": "^4.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "husky": "^7.0.2",
    "less": "^4.1.1",
    "lint-staged": "^11.1.2",
    "mockjs": "^1.1.0",
    "patch-package": "^6.4.7",
    "postcss-plugin-px2rem": "^0.8.1",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.3.2",
    "svgo": "^2.5.0",
    "typescript": "^4.3.2",
    "vite": "^2.5.2",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-svg-icons": "^1.0.4"
  },
  "browserslist": {
    "production": [
      "> 0.5%",
      "last 2 versions",
      "ie >= 10",
      "iOS >= 8",
      "Android >= 4"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*{js,jsx,ts,tsx}": [
      "npm run lint:src",
      "git add"
    ]
  },
  "eslintIgnore": [
    "node_modules",
    "dist",
    "public",
    "postcss.config.js",
    "src/**/*.d.ts",
    "src/**/*.js"
  ],
  "prettierIgnore": []
}
