{
  "name": "vite-ant-mobile",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "tsc:init": "tsc --init",
    "readme:v1": "echo 'Vite starts the react service'",
    "dev": "vite --host",
    "preview": "vite preview --host",
    "build": "tsc && vite build",
    "readme:v2": "echo 'test lint...'",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.vue --ignore-pattern \"bad.*\" --max-warnings 0",
    "lint:fix": "eslint ./src --ext .js,.jsx,.ts,.tsx,.vue --ignore-pattern \"bad.*\" --fix",
    "prettier": "prettier -l \"./src/*\"",
    "prettier:fix": "prettier --write -l \"./src/*\""
  },
  "dependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "babel-eslint": "^10.1.0",
    "commitizen": "^4.2.4",
    "eslint": "^7.32.0",
    "eslint-config-alloy": "^4.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "typescript": "^4.3.2",
    "vite": "^2.5.2"
  },
  "browserslist": {
    "production": [
      "> 0.5%",
      "last 2 versions",
      "ie >= 10",
      "iOS >= 8",
      "Android >= 4"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*{js,jsx,ts,tsx}": [
      "npm run lint",
      "git add"
    ]
  },
  "eslintIgnore": [
    "node_modules",
    "dist",
    "public",
    "*.d.ts"
  ],
  "prettierIgnore": []
}
